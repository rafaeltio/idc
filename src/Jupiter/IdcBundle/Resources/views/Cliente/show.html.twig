{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Cliente</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ entity.tipo }}</td>
            </tr>
            <tr>
                <th>Nome</th>
                <td>{{ entity.nome }}</td>
            </tr>
            <tr>
                <th>Razaosocial</th>
                <td>{{ entity.razaoSocial }}</td>
            </tr>
            <tr>
                <th>Telefonefixo</th>
                <td>{{ entity.telefoneFixo }}</td>
            </tr>
            <tr>
                <th>Telefonecelular1</th>
                <td>{{ entity.telefoneCelular1 }}</td>
            </tr>
            <tr>
                <th>Telefonecelular2</th>
                <td>{{ entity.telefoneCelular2 }}</td>
            </tr>
            <tr>
                <th>Cpf</th>
                <td>{{ entity.cpf }}</td>
            </tr>
            <tr>
                <th>Cnpj</th>
                <td>{{ entity.cnpj }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('cliente') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('cliente_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
